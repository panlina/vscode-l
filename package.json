{
    "name": "vscode-l",
    "displayName": "l",
    "description": "l language support",
    "version": "0.0.0",
    "engines": {
        "vscode": "^1.61.0"
    },
    "author": "Jack Lu <jacklu@jacklu.me>",
    "license": "ISC",
    "categories": [
        "Programming Languages"
    ],
    "contributes": {
        "languages": [{
            "id": "l",
            "aliases": ["l", "l"],
            "extensions": [".l"],
            "configuration": "./language-configuration.json"
        }],
        "grammars": [{
            "language": "l",
            "scopeName": "source.l",
            "path": "./syntaxes/l.tmLanguage.json"
        }]
    }
}